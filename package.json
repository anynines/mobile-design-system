{
  "name": "@anynines/mobile-design-system",
  "version": "0.1.0",
  "main": "node_modules/expo/AppEntry.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "expo start",
    "build": "yarn rm:build && npm run build:js",
    "build:js": "babel --config-file ./_.babelrc src --out-dir dist --extensions \".js,.jsx\" --source-maps inline",
    "rm:build": "rm -rf ./dist/",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "storybook": "expo start --ios"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.11.1",
    "expo": "~38.0.2",
    "expo-status-bar": "~1.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
    "react-native-elements": "^2.0.2",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-vector-icons": "7.1.0",
    "react-native-web": "~0.13.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addons": "^5.3.19",
    "@storybook/react-native": "^5.3.19",
    "@storybook/react-native-server": "^5.3.19",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-expo": "^8.2.3",
    "concurrently": "5.2.0"
  },
  "private": true
}
